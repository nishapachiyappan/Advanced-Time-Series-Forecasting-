def evaluate(y_true, y_pred, label):
    rmse = np.sqrt(mean_squared_error(y_true, y_pred))
    mae = mean_absolute_error(y_true, y_pred)
    print(f"{label:15s} | RMSE = {rmse:.4f} | MAE = {mae:.4f}")

print("\n===== FORECAST PERFORMANCE =====")
evaluate(Y_obs, Y_pred_kf, "Kalman Filter")
evaluate(Y_obs, Y_pred_naive, "Naive Baseline")
evaluate(Y_obs, Y_pred_var, "VAR Baseline")
